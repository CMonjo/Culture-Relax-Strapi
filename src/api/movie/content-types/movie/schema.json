            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "public": {
            "type": "enumeration",
            "enum": [
                "Tout public",
                "Situation de handicap"
            ],
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "featured_image": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images"
            ],
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "trailer": {
            "type": "string",
            "required": false,
