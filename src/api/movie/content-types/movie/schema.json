        "title": {
            "type": "string",
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "description": {
            "type": "text",
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "description_falc": {
            "type": "text",
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "duration": {
            "type": "time",
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "actor": {
            "type": "text",
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "director": {
            "type": "text",
            "required": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            }
        },
        "release": {
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "type": "date",
            "required": true
        },
        "type": {
            "type": "enumeration",
            "enum": [
                "animation",
                "tvshow",
                "documentary"
